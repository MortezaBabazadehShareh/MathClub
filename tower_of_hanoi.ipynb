   "source": [
    "### The tower of hanoi recursive function \n",
    "function hanoi(a::Int, b::Int, c::Int, n::Int)\n",
    "    if n==1\n",
    "        print(\"$a ---> $c\\n\")\n",
    "    else\n",
    "        hanoi(a, c, b, n-1)\n",
    "        print(\"$a ---> $c\\n\")\n",
    "        hanoi(b, a, c, n-1)\n",
    "    end\n",
    "end"
   ]
